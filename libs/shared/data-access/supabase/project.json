{
  "name": "supabase",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/services/supabase/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/services/supabase",
        "main": "libs/services/supabase/src/index.ts",
        "tsConfig": "libs/services/supabase/tsconfig.lib.json",
        "assets": [
          "libs/services/supabase/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/services/supabase/jest.config.ts"
      }
    }
  },
  "tags": []
}